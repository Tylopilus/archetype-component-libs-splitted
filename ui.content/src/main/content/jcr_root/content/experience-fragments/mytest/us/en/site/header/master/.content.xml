<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[]"
        cq:template="/conf/mytest/settings/wcm/templates/xf-web-variation"
        cq:xfMasterVariation="{Boolean}true"
        cq:xfVariantType="web"
        jcr:primaryType="cq:PageContent"
        jcr:title="Header"
        sling:resourceType="mytest/components/xfpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="mytest/components/container"
            layout="simple">
            <navigation
                jcr:primaryType="nt:unstructured"
                sling:resourceType="mytest/components/navigation"
                collectAllPages="false"
                navigationRoot="/content/mytest/us/en"
                skipNavigationRoot="false"
                structureDepth="1"/>
            <languagenavigation
                jcr:primaryType="nt:unstructured"
                sling:resourceType="mytest/components/languagenavigation"
                navigationRoot="/content/mytest/us"
                structureDepth="2"/>
            <search
                jcr:primaryType="nt:unstructured"
                sling:resourceType="mytest/components/search"
                searchRoot="/content/mytest/us"/>
        </root>
    </jcr:content>
</jcr:root>
